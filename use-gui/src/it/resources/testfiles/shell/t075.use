model testEnums

enum Enum1 {
  Value1,
  Value2
}

enum Enum2 {
  Value1,
  Value2
}

class Test
  attributes
    enum1 : Enum1
	enum2 : Enum2
  constraints
    inv testValue1:
	  self.enum1 = Enum1::Value1 or self.enum1 = Enum1::Value2
	inv testValue2:
	  self.enum2 = Enum2::Value1 or self.enum2 = Enum2::Value2
end
