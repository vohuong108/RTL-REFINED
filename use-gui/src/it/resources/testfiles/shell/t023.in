!create a:A
!create b:B
!create c:C
!create d:D
!create e:E

# sets

?Set{a,b}
*-> Set{a,b} : Set(A)
:Set{a,b}
*-> Set(A)

?let x:Set(OclAny) = Set{a,b} in x
*-> Set{a,b} : Set(A)
:let x:Set(OclAny) = Set{a,b} in x
*-> Set(OclAny)

?Set{a,b,e}
*-> Set{a,b,e} : Set(OclAny)
:Set{a,b,e}
*-> Set(OclAny)

?Set{a,b,e}->reject(x|x = e)
*-> Set{a,b} : Set(A)
:Set{a,b,e}->reject(x|x <> e)
*-> Set(OclAny)

