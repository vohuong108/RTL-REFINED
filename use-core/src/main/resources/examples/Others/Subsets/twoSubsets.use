model SimpleSubset

class A
end

class B
end

class C < A
end

class D < B
end

class E < A
end

class F < B
end

association ab between
  A[*] role a union
  B[*] role b union
end

association cd between
  C[*] role c subsets a
  D[*] role d subsets b
end

association ef between
  E[1..10] role e subsets a
  F[0..5] role f subsets b
end